#--------------------[EVENTS BY PLAYER NE PAS TOUCHER]--------------------

on break of leaves:
	if "{@Pommes}" is "y":
		chance of {@PommesPrct}%:
			drop 1 apple
		if "{@GoldApple}" is "y":
			chance of {@GoldApplePrct}%:
				drop 1 golden apple

on break of oak leaves:
	if "{@Pommes}" is "y":
		stop
	else:
		chance of {@PommesPrct}%:
			drop 1 apple
		if "{@GoldApple}" is "y":
			chance of {@GoldApplePrct}%:
				drop 1 golden apple
				
on death of ghast:
	if "{@Ghast}" is "y":
		chance of {@GhastPrct}%:
			drop golden ingot at location of victim
	
on death of zombie pigman:
	if "{@Pigs1}" is "y":
		chance of {@Pigs1Prct}%:
			drop golden apple at location of victim
			
on death of pig:
	if "{@MobsLeather}" is "y":
		chance of {@MobsLeatherPrct}%:
			drop leather at location of victim
			
on death of sheep:
	if "{@MobsLeather}" is "y":
		chance of {@MobsLeatherPrct}%:
			drop leather at location of victim
			
on death of zombie:
	if "{@MobsLeather}" is "y":
		chance of {@MobsLeatherPrct}%:
			drop leather at location of victim
			
on death of chicken:
	if "{@MobsLeather}" is "y":
		chance of {@MobsLeatherPrct}%:
			drop leather at location of victim
			
on death of magma cube:
	if "{@MagmaCream}" is "y":
		chance of {@MagmaCreamPrct}%:
			drop magma cream at location of victim
			
on portal enter:
	wait 5 tick
	execute console command "/execute %player% ~ ~ ~ /gamerule commandBlockOutput false"
	execute console command "/execute %player% ~ ~ ~ /gamerule naturalRegeneration false"
			
on rightclick with compass:
    if "{@Compass}" is "n":
        send "{@logo2}§c Cette action n'est pas disponible."

on rightclick with compass:
    if "{@Compass}" is "y":
        set {_distance} to 1999999
        loop all players:
            loop-player is not the player
            loop-player's gamemode is survival
            set {_t} to distance between loop-player and player
            {_t} is less than {_distance}
            set {_distance} to {_t}
            set {_p} to loop-player
            set {_Coord} to location of {_p}
        if {_p} is not set:
            send "§cAucun joueur détécté."
        else: 
            set the player's compass target to location of {_Coord}
            send "§9Le joueur §4%{_p}% §9est à §4%{_distance}% §9blocks de distance"
            damage player by 0.5 hearts
		
on item spawn of 370:
	if "{@Regen}" is "n":
		cancel event
		
on item spawn of 348:
	if "{@LvL2}" is "n":
		cancel event
	
on craft:
	if "{@DiamondC}" is "y":
		#if event-item is diamond pickaxe or diamond axe or diamond shovel or diamond sword or diamond hoe or enchanting table or diamond chestplate or diamond helmet or diamond leggings or diamond boots or enchantment table:
