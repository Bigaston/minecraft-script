every 1 seconds:
	if {uhc.start} is set:
		if {uhc.pause} is not set:
			add -1 to {uhc.sec}
	if {uhc.sec} is -1:
		add -1 to {uhc.min}
		set {uhc.sec} to 59
		if {uhc.min} is 0:
			broadcast "{@logo2} §eIl reste §61§e minutes avant la fin de l'épisode!"
			execute console command "/execute @a ~ ~ ~ /playsound mob.wolf.growl @p "
	if {uhc.min} is -1:
		broadcast "{@logo2} §eFin de l'épisode §6%{uhc.ep}%"
		execute console command "/execute @a ~ ~ ~ /playsound mob.wolf.growl @p "
		add 1 to {uhc.ep}
		set {uhc.min} to {@Minutes}
		set {uhc.sec} to 0
		if {uhc.ep} is {@PvP}:
			broadcast "{@logo2} §cLe §4PvP§c est actifs, Bonne chance !"
			execute console command "/execute @a ~ ~ ~ /playsound mob.wolf.growl @p "
			delete {uhc.nopvp}
		if "{@EternalDay}" is "y":		
			if {uhc.ep} is {@EternalDayEp}:
				execute console command "/gamerule doDaylightCycle false"
				execute console command "/time set 6000"
				broadcast "{@logo2} §eL'§6eternalDay§e est actif, la nuit n'existe plus."
				
				
