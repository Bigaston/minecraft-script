			if {uhc.team.%loop-player%} is "jaune":
				set the loop-player's tablist name to "§e%loop-player% §6%{uhc.health.%loop-player%}%"
			if {uhc.team.%loop-player%} is "vertclair":
				set the loop-player's tablist name to "§a%loop-player% §6%{uhc.health.%loop-player%}%"
			if {uhc.team.%loop-player%} is "vert":
