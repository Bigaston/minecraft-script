#--------------------[OPTIONS DE JEU]--------------------

options:
	logo: §6FuckingFight §5VI
	logo2: §8[§6FuckingFight§8]§r
	TempsAvantDégâts: 60 #En secondes
	#Zone de TP (-x/+y/+z)
	x: -1000
	y: 250
	z: 1000
	#Taille de la bordure (z*2)
	Border: 2000
	#Pommes qui peuvent spawn dans touts les feuillages (y/n)
	Pommes: y
	#Pourcentage de chance de les avoirs
	PommesPrct: 10
	#Pommes en or qui peuvent drop en cassant des feuilles (y/n)
	GoldApple: y
	#Pourcentage de les avoirs
	GoldApplePrct: 2.5
	#Degats a chaque diamant miné (y/n)
	BloodyDiamond: y
	#degats infligés pour le bloody diamond:
	DiamondDamages: 0.25
	#Systeme de sac a dos (y/n)
	Backpack: n
	#Tous les animaux droppent du cuir (y/n)
	MobsLeather: y
	#Pourcentage de chance de drop du cuir
	MobsLeatherPrct: 20
	#Taux de drop de magma cream augmenté (y/n)
	MagmaCream: n
	#Pourcentage de drop
	MagmaCreamPrct: 50
	#Eternalday (y/n)
	EternalDay: y
	#EternalDay à partir de quel épisode
	EternalDayEp: 5
	#Chronomètre
	Minutes: 15
	#Jour où le PVP est actif
	PvP: 2
	#Kick joueur à la mort (y/n)
	Kick: y
	#FriendlyFire (y/n)
	FriendlyFire: y
	#Pigmens qui droppent des pommes d'or (y/n)
	Pigs1: n
	#Pourcentage de drops
	Pigs1Prct: 5
	#Pigmens qui droppent des diamants (y/n)
	Pigs2: n
	#Pourcentage de drops
	Pigs2Prct: 5
	#Ghasts qui droppent de l'or (y/n)
	Ghast: n
	#Pourcentage de drops
	GhastPrct: 5
	#Potions de Regen (y/n)
	Regen: y
	#Potions de level 2 (y/n)
	LvL2: n
	#Autoriser la canne a pêche (y/n)
	Canap: n
	#Autoriser les shears (y/n)
	Shears: n
	#1 diamant économisé pour les crafts (y/n)
	DiamondC: y
	#Pourcentage de chances
	DiamondCPrct: 5
	#1 or économisé pour les crafts (y/n)
	GoldC: y
	#Pourcentage de chances
	GoldCPrct: 5
	#Pomme en diamant (y/n)
	DiamondApple: y
	#Boussole (y/n)
	Compass: n
	#Spawn avec un bateau (y/n)
	BoatSpawn: y
	#Spawn avec une pelle en pierre (y/n)
	StoneShovel: y
	#Spawn avec une hache en pierre (y/n)
	StoneAxe: y 
	#Spawn avec une houe en pierre (y/n)
	StoneHoe: n 
	#Spawn avec une pioche en pierre (y/n)
	StonePickaxe: y 
	#Spawn avec une épée en pierre (y/n)
	StoneSword: y
	#Spawn avec de la nourriture (y/n)
	GaveMeat: y
	#Combien de steak	
	GaveMeatNb: 32
	#Nether activé (y/n)
	Nether: n
	#Double barre de vie (y/n)
	LifeX2: y
	#Message de kick si mort:
	DeathMSG: §6GG !
	
#--------------------[CODE DU PLUGIN NE PAS TOUCHER]--------------------

#Ce plug in a été codé pour permettre a toute personne de pouvoir créer
#sa partie d'UHC entre amis. Si vous utilisez ce plug in dans une vidéo
#Merci de mentionner dans la vidéo et en descriptif les codeurs
#(Xenakiff, MrBigaston, lexmad13009) ainsi que leur chaîne youtube
#respectives. Pour l'utilisation de ce plug in, modifiez seulement
#la partie intituée "OPTIONS DE JEU", et veillez a ce que tous
#les paramètres soient utilisable par le skript. Pour eviter tout
#beugs au niveau des TP, évitez de toucher les ligne 7,8,9,11.
#Nous vous remercions d'avoir téléchargé notre plug in
#et espérons avoir de bons retours de votre part.
#
#BON GAME A TOUS !

#--------------------[EVENTS BY SKRIPT NE PAS TOUCHER]--------------------

	
on skript load:
	delete {uhc.start}
	set {uhc.nbplayer} to 0
	set {uhc.min} to {@Minutes}
	set {uhc.sec} to 0
	set {uhc.ep} to 1
	execute console command "/gamerule sendCommandFeedback false"
	execute console command "/gamerule commandBlockOutput false"
	execute console command "/gamerule naturalRegeneration false"
	execute console command "/gamerule doDaylightCycle false"
	execute console command "/time set 6000"
	execute console command "/weather clear"
	set {uhc.nodamage} to 1
	set {uhc.nopvp} to 1
	if "{@DiamondApple}" is "y":
		register new shaped recipe for 322:1 using gold block, diamond block, gold block, diamond block, apple, diamond block, gold block, emerald block, gold block
	if "{@Compass}" is "y":
		register new shaped recipe for 345 using bone, iron ingot, string, iron ingot, diamond, iron ingot, magma cream, iron ingot, spider eye
	
on first join:
	teleport player to {uhc.spawn}
	set {uhc.kill.%player%} to 0
	set the player's gamemode to adventure
	send "{@logo2} §4/!\ §2Plug In lancé correctement ✔ §4/!\"
	send "{@logo2} §2Bon jeu !"
	execute console command "/gamerule commandBlockOutput false"
	execute console command "/gamerule naturalRegeneration false"
	send player title "{@logo}" with subtitle "§eBienvenue"

on join:
	set join message to "§7[§2+1§7] %player%"
	send "{@logo2} §5Le §6/rules §5s'impose pour lire les règles"
	send "§4[ADMINS] §5Utilisez la commande §6/uhc help §5pour afficher les commandes disponibles"
	if {uhc.spec.%player%} is set:
		execute console command "/gamemode 3 %player%"
	if {uhc.start} is not set:
		delete {uhc.spec.%player%}
		set {uhc.kill.%player%} to 0
	if "{@LifeX2}" is "y":
		set rf max hp of player to 40
		if {uhc.start} is not set:
			wait 5 ticks
			heal the player
	if "{@LifeX2}" is "n":
		set rf max hp of player to 20
		if {uhc.start} is not set:
			wait 5 ticks
			heal the player

on respawn:
	if {uhc.spec.%player%} is set:
		execute console command "/gamemode 3 %player%"

on quit:
	set quit message to "§7[§4-1§7] %player%"
	add -1 to {uhc.nbplayer}

on damage:
	if {uhc.nodamage} is set:
		if victim is a player:
			cancel event
		else:
			stop
	if {uhc.team.%victim%} is {uhc.team.%attacker%}:
		if "{@FriendlyFire}" is "n":
			cancel event
			message "{@logo}§c C'est ton mate !" to attacker
	if {uhc.nopvp} is set:
		if attacker is a player:
			if victim is a player:
				cancel event
				message "{@logo2} §eLe PvP est §6désactivé §e!" to attacker
	if {uhc.pause} is set:
		cancel event
		message "{@logo2} §eLa partie est en pause."

On Hunger Meter Change:
	if {uhc.start} is not set:
		cancel event

on break:
	if {uhc.pause} is set:
		cancel event
		send "{@logo2} §eLa partie est en pause."

on place:
	if {uhc.pause} is set:
		cancel event
		send "{@logo2} §eLa partie est en pause."

on chat:
	if {uhc.spec.%player%} is set:
		cancel event
	if {uhc.team.%player%} is "bleuclair":
		set message format to "§9<%player%> §r%message%"
	if {uhc.team.%player%} is "cyan":
		set message format to "§3<%player%> §r%message%"
